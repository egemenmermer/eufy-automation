# Real Test Environment Configuration
# Copy this to .env after setting up real credentials

# Server Configuration
PORT=3001
NODE_ENV=development

# Automation Settings (faster for testing)
POLL_INTERVAL_MINUTES=1
UNLOCK_DURATION_MINUTES=5
CALENDAR_POLL_INTERVAL_SECONDS=30

# Google Calendar API (REAL - Set up required)
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./credentials/google-service-account.json
GOOGLE_CALENDAR_ID=your-calendar-id@group.calendar.google.com

# Email Configuration (REAL - Gmail SMTP)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-16-char-app-password
EMAIL_FROM=your-email@gmail.com
EMAIL_FROM_NAME=Eufy Automation Test

# Eufy Smart Lock (MOCK - keep these as test values)
EUFY_USERNAME=test@example.com
EUFY_PASSWORD=test-password
EUFY_DEVICE_SERIAL=TEST123456

# System Configuration
LOCK_DURATION_MINUTES=5
TIMEZONE=America/New_York

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/automation.log

# API Configuration
ENABLE_API=true 